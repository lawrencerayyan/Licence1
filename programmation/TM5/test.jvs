void main() {
    int[] tab = new int[100];
    int n = readInt("Quel est la valeur de n ?");
    for (int i = 0; i < tab.length; i++) {
        tab[i] = readInt("Entrez un nombre");
    }
    for (int z = 0; z < tab.length; z++) {
        if((tab[z]%n)!=0) {
            tab[z]=-1;
        }
    }
    for (int u = 0; u < tab.length; u++) {
        println(tab[u]);
    }
}
