int [] tabHasard(int n, int h) {
    int[] tab = new int[n];
    for (int i = 0; i < tab.length; i++) {
        int a = random(0, h);
        tab[i] = a;
    }

    // for (int i = 0; i < tab.length; i++) {
    //     println(tab[i]);
    // }
    return tab ;
}
void main() {
    int n = readInteger("la taille de tableau");;
    int h = readInteger("le nombre au hasard"); ;

    tabHasard(n , h);
    
}