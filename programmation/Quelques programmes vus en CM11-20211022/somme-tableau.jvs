int somme_tableau(int[] arg) {
    int somme = 0;
    for (int i = 0; i < arg.length; i++) {
        somme = somme + arg[i];
    }
    return somme;
    // somme = arg[0]+arg[1]+arg[2]+...
}

void main() {
    int[] tab = new int[] {10, 20, 35};
    int s = 0;
    s = somme_tableau(tab); // erreur de conception mais pas syntaxique !
    println("la somme des elements du tableau est " + s);
}
