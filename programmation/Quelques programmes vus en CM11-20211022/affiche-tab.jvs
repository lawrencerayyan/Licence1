    println("valeur pour indice 2 : "+arg[2]);
    ....
    print(arg[3]);
    print(arg[4]);
    print(arg[5]);
    print(arg[6]);
    ....*/
}

void main() {
    int[] tab = new int[] {1, 2, 3, 4, 5, 6};
