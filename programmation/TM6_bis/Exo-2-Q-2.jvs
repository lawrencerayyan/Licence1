int[] tabMultiple(int[] t, int n) {
    if (t.length>0){
        int[] tab = new int[((t.length-1)/n)+1];
        int index = 0;
        for (int i = 0; i <= t.length-1; i=i+n) {
            tab[index] = t[i];
            index++;
        }
        return tab;
    } else {
        int[] tab = new int[0];
        return tab;
    }
}

