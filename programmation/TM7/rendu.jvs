int[] somme_tableau(int[] tab1, int[] tab2) {
    int[] result = new int[tab1.length];
    if (tab1.length == tab2.length) {
        for (int i = 0; i < tab1.length; i++) {
            result[i] = tab1[i] + tab2[i];
        }
    } else {
        int[] fail = new int[1];
        return fail;
    }
    return result;
}

